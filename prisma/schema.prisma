generator client {
  provider          = "prisma-client-js"
  previewFeatures   = ["referentialActions"]
  shadowDatabaseUrl = ""
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id        String   @id @default(uuid())
  email     String   @unique
  password  String
  createdAt DateTime @default(now())
}
